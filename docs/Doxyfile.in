PROJECT_NAME            = @TARGET@
PROJECT_NUMBER          = @TARGET_VERSION@
OUTPUT_DIRECTORY        = @CMAKE_CURRENT_BINARY_DIR@/doc_doxygen/
INPUT                   = @CMAKE_SOURCE_DIR@/a3voda/src/ @CMAKE_SOURCE_DIR@/a3voda/inc/ @CMAKE_SOURCE_DIR@/README_RU.md
USE_MDFILE_AS_MAINPAGE  = README_RU.md
EXAMPLE_PATH            = @CMAKE_SOURCE_DIR@/src/

RECURSIVE               = YES

EXTRACT_LOCAL_CLASSES   = YES
CLASS_GRAPH             = NO
COLLABORATION_GRAPH     = NO
CALL_GRAPH              = NO
CALLER_GRAPH            = NO
INCLUDE_GRAPH           = NO
INCLUDED_BY_GRAPH       = NO
DIRECTORY_GRAPH         = NO
HIDE_SCOPE_NAMES        = YES
SHOW_USED_FILES         = NO
SHOW_INCLUDE_FILES      = NO
SHOW_NAMESPACES         = NO
HIDE_UNDOC_MEMBERS      = YES
HIDE_UNDOC_CLASSES      = YES
ALPHABETICAL_INDEX      = NO
COMPACT_LATEX           = YES

OUTPUT_LANGUAGE         = Russian
EXTRA_PACKAGES          = [russian]{babel}


